{
  "name": "nuxo",
  "version": "0.1.0",
  "main": "public/main.js",
  "description": "DuoNuxo Api Sii",
  "author": "Duoloon",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "nodemon public/server",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron .",
    "publish": "yarn build && electron-builder -c.extraMetadata.main=build/main.js -p always",
    "postinstall": "install-app-deps"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^11.2.1",
    "electron-builder": "^22.11.7",
    "prettier": "^2.2.1",
    "wait-on": "^5.2.1"
  },
  "dependencies": {
    "@electron/remote": "^1.0.2",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.5.0",
    "@mui/lab": "^5.0.0-alpha.74",
    "@mui/material": "^5.5.1",
    "@mui/x-data-grid": "^5.6.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@vscode/sqlite3": "^5.0.7",
    "autosuggest-highlight": "^3.2.1",
    "axios": "^0.24.0",
    "body-parser": "^1.19.2",
    "bookshelf": "^1.2.0",
    "cookie-parser": "^1.4.6",
    "electron-is-dev": "^1.2.0",
    "electron-updater": "^4.3.9",
    "electron-window-state-manager": "^0.3.2",
    "express": "^4.17.3",
    "jotai": "^1.6.1",
    "knex": "^0.95.10",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "notistack": "^2.0.3",
    "path": "^0.12.7",
    "react": "^17.0.1",
    "react-charts": "^3.0.0-beta.36",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.33.6",
    "react-resizable": "^3.0.4",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.1",
    "rut.js": "^2.1.0",
    "sequelize": "^6.17.0",
    "sqlite3": "^5.0.2",
    "web-vitals": "^0.2.4",
    "xlsx": "^0.18.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "pdfreader": "^1.2.14",
    "puppeteer": "^13.3.2",
    "uuid": "^8.3.2",
    "xml2js": "^0.4.23"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Duoloon/electron-nuxo"
  },
  "bugs": {
    "url": "https://github.com/Duoloon/electron-nuxo/issues"
  },
  "homepage": "./",
  "build": {
    "appId": "com.github.Duoloon.electron-nuxo",
    "productName": "electron-nuxo",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Duoloon",
        "repo": "electron-nuxo"
      }
    ],
    "linux": {
      "category": "your.app.category.type",
      "target": "AppImage"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
